{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "info",
    "title": "Software common information",
    "properties": {
        "email": {
            "type": "string",
            "pattern": "\\w{1,}@\\w{1,}[.]{1}\\w{1,}"
        },
        "url": {
            "type": "string",
            "pattern": "\\w{1,}[.]{1}\\w{1,}[.]{0,}\\w{0,}"
        }
    }
}